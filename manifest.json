{
  "_version": "1.12.0",
  "sap.app": {
    "id": "sap_ui5",
    "type": "application",
    "title": "Checklist Application",
    "description": "Enterprise Checklist Management",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
       "uri": "http://localhost:8000/sap/opu/odata/sap/Z_UI5_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "viewName": "sap_ui5.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.71.70",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.layout": {},
        "sap.ui.comp": {},
        "sap.ui.table": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "mainService": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "mainService",
        "settings": {
          "useBatch": true,
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "defaultOperationMode": "Server",
          "deferredGroups": [
            "changes",
            "autosave"
          ],
          "changeGroups": {
            "*": {
              "groupId": "changes",
              "single": false
            },
            "LockAcquireType": {
              "groupId": "locks",
              "single": true
            },
            "LockHeartbeatType": {
              "groupId": "locks",
              "single": true
            },
            "LockReleaseType": {
              "groupId": "locks",
              "single": true
            }
          }
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap_ui5.i18n.i18n",
          "supportedLocales": [
            "",
            "en",
            "en_US",
            "ru"
          ],
          "fallbackLocale": "en"
        }
      },
      "data": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "state": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "layout": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "cache": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "masterData": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "mpl": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap_ui5.util.FlexibleRouter",
        "viewType": "XML",
        "viewPath": "sap_ui5.view",
        "controlId": "fcl",
        "controlAggregation": "beginColumnPages",
        "async": true,
        "fclControlId": "fcl"
      },
      "routes": [
        {
          "pattern": "",
          "name": "search",
          "target": [
            "search"
          ],
          "layout": "OneColumn",
          "targetColumn": "begin"
        },
        {
          "pattern": "detail/{id}",
          "name": "detail",
          "target": [
            "search",
            "detail"
          ],
          "layout": "TwoColumnsMidExpanded",
          "targetColumn": "mid"
        },
        {
          "pattern": "detail/{id}/{layout}",
          "name": "detailLayout",
          "target": [
            "search",
            "detail"
          ]
        }
      ],
      "targets": {
        "search": {
          "viewName": "Search",
          "viewLevel": 1,
          "controlAggregation": "beginColumnPages",
          "targetColumn": "begin",
          "layout": "OneColumn"
        },
        "detail": {
          "viewName": "Detail",
          "viewLevel": 2,
          "controlAggregation": "midColumnPages",
          "targetColumn": "mid",
          "layout": "TwoColumnsMidExpanded"
        }
      }
    },
    "config": {
      "backendMode": "real",
      "uiContractVersion": "1.0.0"
    },
    "flexEnabled": false
  }
}
