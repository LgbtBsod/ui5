{
  "sap.app": {
    "id": "theme7",
    "type": "application"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "theme7.view.App",
      "type": "XML"
    },
    "dependencies": {
      "libs": {
        "sap.m": {},
        "sap.f": {},
        "sap.ui.layout": {},
        "sap.ui.comp": {}
      }
    },
    "models": {
      "dataModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "stateModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "layoutModel": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "controlId": "fcl",
        "async": true,
        "viewPath": "theme7.view"
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": [
            "master"
          ]
        },
        {
          "pattern": "object/{id}",
          "name": "object",
          "target": [
            "master",
            "object"
          ]
        },
        {
          "pattern": "detail/{id}",
          "name": "detail",
          "target": [
            "master",
            "object",
            "detail"
          ]
        }
      ],
      "targets": {
        "master": {
          "viewName": "Master",
          "controlAggregation": "beginColumnPages"
        },
        "object": {
          "viewName": "Object",
          "controlAggregation": "midColumnPages"
        },
        "detail": {
          "viewName": "Detail",
          "controlAggregation": "endColumnPages"
        }
      }
    }
  }
}