{
  "_version": "1.12.0",
  "sap.app": {
    "id": "sap_ui5",
    "type": "application",
    "title": "Checklist Application",
    "description": "Enterprise Checklist Management",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mockGateway": {
        "uri": "http://localhost:8000",
        "type": "REST"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "viewName": "sap_ui5.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.71.70",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.layout": {},
        "sap.ui.comp": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap_ui5.i18n.i18n"
        }
      },
      "data": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "state": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "layout": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "cache": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "masterData": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "mpl": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sap_ui5.view",
        "controlId": "fcl",
        "controlAggregation": "beginColumnPages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "search",
          "target": [
            "search"
          ],
          "layout": "OneColumn"
        },
        {
          "pattern": "detail/{id}",
          "name": "detail",
          "target": [
            "search",
            "detail"
          ],
          "layout": "TwoColumnsMidExpanded"
        },
        {
          "pattern": "object/{id}",
          "name": "object",
          "target": [
            "search",
            "object"
          ],
          "layout": "TwoColumnsMidExpanded"
        }
      ],
      "targets": {
        "search": {
          "viewName": "Search",
          "viewLevel": 1,
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "viewName": "Detail",
          "viewLevel": 2,
          "controlAggregation": "midColumnPages"
        },
        "object": {
          "viewName": "Object",
          "viewLevel": 2,
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "config": {
      "backendMode": "real"
    }
  }
}
